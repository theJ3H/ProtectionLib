		</dependency>

		<dependency>
			<groupId>com.wasteofplastic</groupId>
			<artifactId>askyblock</artifactId>
			<version>3.0.9.4</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/lib/askyblock.jar</systemPath>
